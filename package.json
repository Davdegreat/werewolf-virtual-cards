{
  "name": "werewolf-virtual-cards",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint src/**/*.ts",
