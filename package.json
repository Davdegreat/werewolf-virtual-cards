    "android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'"
  },
  "dependencies": {
    "@types/jest": "^19.2.3",
    "@types/react": "^15.0.24",
    "@types/react-native": "^0.44.3",
    "babel-plugin-transform-inline-environment-variables": "0.0.2",
    "concurrently": "^3.4.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.6",
    "pusher-js": "^4.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "0.43.2",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-device-info": "^0.10.2",
    "react-native-qrcode": "^0.2.3",
    "react-native-rest-client": "^0.1.1",
    "react-native-vector-icons": "^4.0.1",
    "react-navigation": "git+https://github.com/react-community/react-navigation.git#7edd9a7",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
